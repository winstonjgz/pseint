Algoritmo Ejercicio16Un2
	Definir ninos, ninas, total Como Entero;
	Escribir 'La cantidad de niños es: ';
	Leer ninos;
	Escribir 'La cantidad de niñas es: ';
	Leer ninas;
	
	total <- ninos + ninas;
	
	Escribir 'El porcentaje de niños es: ',((ninos)/total)*100;
	Escribir 'El porcentaje de niñas es: ',((ninas)/total)*100;
	
FinAlgoritmo

